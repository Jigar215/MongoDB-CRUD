<%= mydata.length %> Records Found
<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Mobile</th>
        <th>Email</th>
        <th>Action</th>
        <th>Action</th>
        <th>Action</th>
    </tr>
<% for(var i=0;i<mydata.length;i++){ %>
    <tr>
        <td><%= i+1 %></td>
        <td><%= mydata[i].uname %></td>
        <td><%= mydata[i].umobile %></td>
        <td><%= mydata[i].uemail %></td>
        <td><a href="/show/<%=mydata[i]._id%>">Show</a></td>
        <td><a href="/edit/<%=mydata[i]._id%>">Edit</a></td>
        <td><a href="/delete/<%=mydata[i]._id%>" onclick="return confirm('Are you sure you want to delete')">Delete</a></td>
    </tr>
<% } %>
</table>